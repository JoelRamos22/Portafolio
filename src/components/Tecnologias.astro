---
import CPU from "../icons/CPU.astro"
import Html from "../icons/Html.astro"
import Css from "../icons/Css.astro"
import IconAstro from "../icons/IconAstro.astro"
import Tailwind from "../icons/Tailwind.astro"
import LIT from "../icons/LIT.astro"
import Javascript from "../icons/Javascript.astro"
import Node from "../icons/Node.astro"
import Mysql from "../icons/Mysql.astro"
import MongoDB from "../icons/MongoDB.astro"
import Express from "../icons/Express.astro"
import Python from "../icons/Python.astro"
import Flask from "../icons/Flask.astro"
import Vs from "../icons/Vs.astro"
import Git from "../icons/Git.astro"
import Bash from "../icons/Bash.astro"
import Linux from "../icons/Linux.astro"
import Ubunto from "../icons/Ubunto.astro"
import Figma from "../icons/Figma.astro"
import Post from "../icons/Post.astro"
import React from "../icons/React.astro"
import Docker from "../icons/Docker.astro"
import Angular from "../icons/Angular.astro"
import Bun from "../icons/Bun.astro"
---
<section id="Proyectos"
class="w-full max-w-3xl mx-auto flex flex-col gap-y-5 py-16 mt-4">
    <h1 class="text-4xl font-semibold flex items-center gap-2">
        Tecnolog√≠as <CPU class="astro"></CPU>
    </h1>
    <div class="grid grid-cols-1 md:grid-cols-2 gap-3 bg-opacity-5">
        <div class="bg-gray-600 bg-opacity-30 rounded-3xl backdrop-blur-lg shadow-md flex flex-col justify-center">
            <h2 class="text-yellow-400 text-2xl font-medium p-4">Frontend</h2>
            <div class="p-4 flex flex-wrap gap-x-10 justify-center gap-y-2">
                <div class="flex flex-col items-center">
                    <Html></Html>
                    <small class="font-normal text-base py-1">HTML</small>
                </div>
                <div class="flex flex-col items-center">
                    <Css></Css>
                    <small class="font-normal text-base py-1">CSS</small>
                </div>
                <div class="flex flex-col items-center">
                    <Tailwind></Tailwind>
                    <small class="font-normal text-base py-1">Tailwind</small>
                </div>
                <div class="flex flex-col items-center">
                    <IconAstro></IconAstro>
                    <small class="font-normal text-base py-1">Astro</small>
                </div>
                <div class="flex flex-col items-center">
                    <LIT class="astro"></LIT>
                    <small class="font-normal text-base py-1">LIT</small>
                </div>
                <div class="flex flex-col items-center">
                    <Javascript class="astro"></Javascript>
                    <small class="font-normal text-base py-1">JavaScript</small>
                </div>
            </div>
        </div>
        <div class="bg-gray-600 bg-opacity-30 rounded-3xl backdrop-blur-lg shadow-md flex flex-col justify-center">
            <h2 class="text-yellow-400 text-2xl font-medium p-4">Backend</h2>
            <div class="p-4 flex flex-wrap gap-x-10 justify-center gap-y-2">
                <div class="flex flex-col items-center">
                    <Node class="astro"></Node>
                    <small class="font-normal text-base py-1">Node.js</small>
                </div>
                <div class="flex flex-col items-center">
                    <Mysql class="astro"></Mysql>
                    <small class="font-normal text-base py-1">MySql</small>
                </div>
                <div class="flex flex-col items-center">
                    <MongoDB class="astro"></MongoDB>
                    <small class="font-normal text-base py-1">MongoDB</small>
                </div>
                <div class="flex flex-col items-center">
                    <Express class="ast"></Express>
                    <small class="font-normal text-base py-1">Express.js</small>
                </div>
                <div class="flex flex-col items-center">
                    <Python class="astro"></Python>
                    <small class="font-normal text-base py-1">Python</small>
                </div>
                <div class="flex flex-col items-center">
                    <Flask class="astro"></Flask>
                    <small class="font-normal text-base py-1">Flask</small>
                </div>
            </div>
        </div>
        <div class="bg-gray-600 bg-opacity-30 rounded-3xl backdrop-blur-lg shadow-md flex flex-col justify-center">
            <h2 class="text-yellow-400 text-2xl font-medium p-4">Herramientas</h2>
            <div class="p-4 flex flex-wrap gap-x-10 justify-center gap-y-2">
                <div class="flex flex-col items-center">
                    <Git></Git>
                    <small class="font-normal text-base py-1">Git</small>
                </div>
                <div class="flex flex-col items-center">
                    <Bash></Bash>
                    <small class="font-normal text-base py-1 mt-1">Bash</small>
                </div>
                <div class="flex flex-col items-center">
                    <Vs></Vs>
                    <small class="font-normal text-base py-1">VS Code</small>
                </div>
                <div class="flex flex-col items-center">
                    <Ubunto></Ubunto>
                    <small class="font-normal text-base py-1 mt-1">Ubuntu</small>
                </div>
                <div class="flex flex-col items-center">
                    <Linux></Linux>
                    <small class="font-normal text-base py-1 mt-1">Linux</small>
                </div>
                <div class="flex flex-col items-center">
                    <MongoDB></MongoDB>
                    <small class="font-normal text-base py-1 mt-1">Compass</small>
                </div>
                <div class="flex flex-col items-center">
                    <Figma></Figma>
                    <small class="font-normal text-base py-1 mt-1">Figma</small>
                </div>
                <div class="flex flex-col items-center">
                    <Post></Post>
                    <small class="font-normal text-base py-1 mt-1">PostgreSql</small>
                </div>
            </div>
        </div>
        <div class="bg-gray-600 bg-opacity-30 rounded-3xl backdrop-blur-lg shadow-md flex flex-col">
            <h2 class="text-yellow-400 text-2xl font-medium p-4">Aprendiendo</h2>
            <div class="p-4 flex flex-wrap gap-x-12 justify-center gap-y-2">
                <div class="flex flex-col items-center">
                    <React></React>
                    <small class="font-normal text-base py-1">React</small>
                </div>
                <div class="flex flex-col items-center">
                    <Docker></Docker>
                    <small class="font-normal text-base py-1">Docker</small>
                </div>
                <div class="flex flex-col items-center">
                    <Angular></Angular>
                    <small class="font-normal text-base py-1 mt-1">Angular</small>
                </div>
                <div class="flex flex-col items-center">
                    <Bun></Bun>
                    <small class="font-normal text-base py-1">Bun</small>
                </div>
            </div>
        </div>
      </div>      
</section>